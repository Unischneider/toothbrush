<div class="container">
  <h1>Users#edit</h1>
  <p>Find me in app/views/users/edit.html.erb</p>

  <%= simple_form_for @user do |f| %>
    <%= f.input :first_name, placeholder: "Tuan" %>
    <%= f.input :last_name, placeholder: "Perera"  %>
    <%= f.input :gender, collection: ["Female", "Male", "Other"]  %>
    <%= f.input :address, placeholder: "Simon-dachstrasse 50" %>
    <%= f.button :submit %>
  <% end %>
</div>
