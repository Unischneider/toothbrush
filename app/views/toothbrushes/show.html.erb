

<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h1>The used toothbrush 3000 txd</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-4 col-xs-offset-2">
      <h2>
        Information about the toothbrush
      </h2>
      <%=  %>
      <ul>
        <li>
          <%= @toothbrush.age %>
        </li>
        <li>
          <%= @toothbrush.people %> %>
        </li>
        <li>
          <%= @toothbrush.material %>
        </li>
        <li>
          <%= @toothbrush.brush_strength %>
        </li>
      </ul>

    </div>
    <div class="col-xs-4 col-xs-offset-2">
      <h2>
        Information about the owner
      </h2>
      <ul>
        <li>
          <%= @toothbrush.user.first_name %>
        </li>
        <li>
          <%= @toothbrush.user.gender %>
        </li>
        <li>
          <%= @toothbrush.user.address %>
        </li>
      </ul>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <h4> <%= @toothbrush.price %> </h4>
    </div>
  </div>
</div>
